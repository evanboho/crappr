<h2>users</h2>

 <ul>
<% @users.each do |user| %>
 
    <li>
    <% if user.profile.present? %>
    <%= user.profile.screen_name %>
    <% end %>
    <%= user.screen_name %><br/>
    <%= link_to 'show', user %>
    <%= link_to 'edit', edit_user_path(user) %>
    <%= link_to 'destroy', user, confirm: 'Are you sure?', method: :delete %>
 
  </li> <br/>  
<% end %>
</ul>

<br />
